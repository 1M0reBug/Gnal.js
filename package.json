{
  "name": "gnal.js",
  "version": "0.0.4",
  "description": "A low impact i18n helper library",
  "main": "dist/gnal.min.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "webpack",
    "build:prod": "webpack && NODE_ENV=production webpack",
    "doc": "jsdoc2md -t jsdoc/README.hbs src/*.js > README.md; echo",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jquagliatini/Gnal.js.git"
  },
  "keywords": [
    "i18n"
  ],
  "author": "jqgl",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jquagliatini/Gnal.js/issues"
  },
  "homepage": "https://github.com/jquagliatini/Gnal.js#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.0",
    "babel-preset-es2015": "^6.18.0",
    "eslint": "4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "2.7.0",
    "jest": "^22.4.2",
    "jsdoc-to-markdown": "^2.0.1",
    "redom": "^3.11.1",
    "webpack": "^1.14.0"
  },
  "eslintConfig": {
    "extends": "airbnb-base",
    "env": {
      "browser": true
    },
    "rules": {
      "indent": [
        "error",
        4
      ],
      "no-plusplus": [
        "error",
        {
          "allowForLoopAfterthoughts": true
        }
      ]
    }
  }
}
